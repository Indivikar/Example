<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fillWidth="false" prefHeight="400" prefWidth="300" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXML.NodesMitPropertiesDatei.MainController">
    <children>
        <Label text="%title" />
      	<Label text="%test" />
      <CheckBox mnemonicParsing="false" text="%checkbox" />
      <TextField fx:id="textField" promptText="%textfieldPromptText" />
      <Button mnemonicParsing="false" onAction="#button" text="%button" />
    </children>
</VBox>
